package arrays;

import java.util.Iterator;

public class MultiDimentional {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a[][] = new int[3][2];
		
		a[0][0] = 10;
		a[0][1] = 12;
		a[1][0] = 15;
		a[1][1] = 4;
		a[2][0] = 3;
		a[2][1] = 2;
		
		for (int i = 0; i < 3; i++) {
			for (int j = 0; j < 2; j++) {
				System.out.print(a[i][j]);
			}
			System.out.println();
		}

	}

}
