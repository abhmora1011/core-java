package arrays;

public class CiscoInterview {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		

		int abc[][] = {{2,4,5},{3,7,7},{1,2,9}};
		
		
		int temp = abc[0][0];
		minColumn = 0;
		
		for (int i = 0; i < 3; i++) {
			
			for (int j = 0; j < 3; j++) {
				
				
				if(temp > abc[i][j]) {
					temp = abc[i][j];
					minColumn = j;
				}
				
			}
			
		}
		System.out.println("The smallest number in the matrix is :" + temp);

	}

}
