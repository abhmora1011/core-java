package arrays;

public class Array1 {

	public static void main(String[] args) {
		
		// A container which store multiple values with same data type
		
		int arr[] = new int[5];
		arr[0] = 1;
		arr[1] = 2;
		arr[2] = 3;
		arr[3] = 4;
		arr[4] = 5;
		
		String arr2[] = {"Abe", "Gale", "Hendrix"}''

	}

}
